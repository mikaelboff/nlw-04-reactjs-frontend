{
    "name": "moveit-next",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint --fix --ext .ts,.tsx",
        "format:prettier": "prettier --write \"src/**/*.{tsx,css}\" --config ./.prettierrc",
        "format": "yarn lint && yarn format:prettier"
    },
    "dependencies": {
        "js-cookie": "^2.2.1",
        "next": "10.0.7",
        "react": "17.0.1",
        "react-dom": "17.0.1"
    },
    "devDependencies": {
        "@types/js-cookie": "^2.2.6",
        "@types/node": "^14.14.31",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "husky": "^5.1.2",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "typescript": "^4.2.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && npm run lint"
        }
    }
}
